{
  "name": "Arknights Clock",
  "description": "Change your browser homepage to Arknights World.",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
